<%= turbo_frame_tag "#{dom_id(video)}_publish_video" ,class: "#{dom_id(video)}_publish_video" do %>
  <% if video.published? %>
    <%= link_to "Unpublish", unpublish_video_path(id: video.id), class: "btn btn-warning btn-block w-100",data: { turbo_method: :post } %>
  <% else %>
    <%= link_to "Publish", publish_video_path(id: video.id), class: "btn btn-secondary btn-block w-100",data: { turbo_method: :post }%>
  <% end %>
<% end %>