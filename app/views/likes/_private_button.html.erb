<%= turbo_frame_tag "#{dom_id(photo)} private_likes" do %>
  <% like_text = like_status ?  "fa-solid fa-heart pink-heart" : "fa-regular fa-heart" %>
  <div class="d-flex align-items-center">
    <%= link_to like_photo_path(id: photo.id),data: { turbo_method: :post } do %>
      <%= content_tag(:i,class: "#{like_text}") do %>
      <% end %>
    <% end %>
    <%= render 'likes/like_count', photo: photo %>
  </div>
<% end %>