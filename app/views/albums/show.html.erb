<p style="color: green"><%= notice %></p>

<%= @album.title %>
<% @album.images.blobs.each do |image|%>
  <%= cl_image_tag(image.key, class: "", :cloud_name => "dvpu5diow", :secure=>"true", :width=>150, :height=>150, :crop=>"fill") %>
<% end %>


<% if current_user == @album.user %>
  <div>
    <%= link_to "Edit this album", edit_album_path(@album) %> |
    <%= link_to "Back to albums", albums_path %>

    <%= button_to "Destroy this album", @album, method: :delete %>
  </div>
<% end %>