<p style="color: green"><%= notice %></p>

<%= @album.title %>

<% if current_user == @album.user %>
  <div>
    <%= link_to "Edit this album", edit_album_path(@album) %> |
    <%= link_to "Back to albums", albums_path %>

    <%= button_to "Destroy this album", @album, method: :delete %>
  </div>
<% end %>